;; ******************************************************************************************************************************
;; modify algorithm to work on stages, without having to read all the sequence into memory, find all matches within a1, a2, a3 
;; then apply search through the entire genome but in said batches, continuously reading them and finding and accumulating match 
;; results will show ability to parse 1,700,000 bps in demo mode --- may be possible to uncover something not in actual genome db
;; ******************************************************************************************************************************


	;; **********************************************************************************************************
	(setf yes nil)
	(setf COUNTMAX 599999)
	(dolist (hps_sequence_num HPS_DNA_ECOLI_GENOME 'DOWNLOAD) 
			(format t "~% RETRIEVING SIGNATURE FROM ENTREZ DB : ~A" 	  HPS_SEQUENCE_NUM )
			(setf *HPS_1DNA_signature_bases*  
		      	(HPS_DNA_WWW_get_sequence		              HPS_SEQUENCE_NUM 
			   	:GENOME_NAME 			     	     "E_COLI_K12"
			   	:OUTPUT_DIR				     "F:/HPS_DATA/HPS_INPUTS/"
			   	:WRITE_1DNA_BASES_SEQUENCE_INTO_THIS_FILE     nil))
			(dotimes (i COUNTMAX 'WAITINGBEFORENEXTFILE) 
				(setf counter 0)
			  	(incf counter)))
	
